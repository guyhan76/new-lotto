<service>
	<id>lottogeneratorservice.exe</id>
	<name>LottoGeneratorService</name>
	<description>Lotto Generator Web Application Service</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>D:\cursor\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\cursor\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>LottoGeneratorService wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<serviceaccount>
		<domain>DESKTOP-NCE5PG0</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>D:\cursor</workingdirectory>
</service>